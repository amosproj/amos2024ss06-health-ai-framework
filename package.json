{
  "name": "ailixir",
  "version": "1.0.0",
  "main": "./src/frontend/index.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev-android": "eas build --platform android --profile dev",
    "build:dev-ios": "eas build --platform ios --profile --profile dev:ios-device",
    "build:dev-ios-simulator": "eas build --platform ios --profile dev:ios-simulator",
    "biome-lint": "biome lint --apply ./src/frontend",
    "biome-format": "biome format --write ./src/frontend",
    "biome-all": "biome check --apply ./src/frontend"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@react-native-community/netinfo": "^11.3.2",
    "expo": "~51.0.9",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.15",
    "expo-font": "~12.0.6",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@biomejs/biome": "1.7.3",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "private": true
}
