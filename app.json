{
  "expo": {
    "name": "AiLixir",
    "slug": "ailixir",
    "owner": "amos-health-ai-framework",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "ailixir",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/02cf6cef-f45f-48b0-a33a-3d0242eaca82"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "backgroundColor": "#FFFFFF",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amos.ailixir",
      "associatedDomains": [
        "applinks:ailixir.page.link"
      ],
      "googleServicesFile": "./google-services.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.amos.ailixir",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "ailixir.page.link"
            }
          ],
          "category":["BROWSABLE", "DEFAULT"]
        }
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/dynamic-links",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "02cf6cef-f45f-48b0-a33a-3d0242eaca82"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
